{
  "name": "web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cucumber-report": "node cucumber-html-report.js",
    "delete-existingJsonReports": "rmdir /S/Q cypress\\reports\\cucumber-json\\*.json",
    "delete-reportFolder": "if exist cypress\\reports rmdir /S/Q cypress\\reports",
    "test": "npx cypress run --e2e --browser chrome ",
    "TestWithReportGeneration": "npm run delete-reportFolder ; npm run test ; npm run cucumber-report ; npm run delete-existingJsonReports",
    "stage-Test": "npx cypress run --config-file stage.config.js",
    "prod-Test": "npx cypress open --config-file prod.config.js",
    "regression-Test" : "npx cypress run --spec=cypress/e2e/feature/Regression",
    "smoke-Test" : "npx cypress run --spec=cypress/e2e/feature/Smoke",
    "testreport" : "(rmdir /s/q cypress\\reports && npm run regression-Test || npm run cucumber-report)",
    "testreport1" : "npm run test || npm run cucumber-report",
    "testreport2" : "if exist cypress\\reports (rmdir /S /Q cypress\\reports && npm run test || npm run cucumber-report) else npm run test || npm run cucumber-report"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.6.5",
    "multiple-cucumber-html-reporter": "^3.6.2"
    
  },
  "dependencies": {
    "cypress-cucumber-preprocessor": "^4.3.1"
    
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/e2e/step_definitions"
  }
}
